#: src/getopt.c:693
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: `%s'는 모호한 옵션입니다\n"

#: src/getopt.c:718
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: `--%s' 옵션은 인수가 필요하지 않습니다\n"

#: src/getopt.c:723
#, c-format
msgid "%s: option `%c%s' doesn't allow an argument\n"
msgstr "%s: `%c%s' 옵션은 인수가 필요하지 않습니다\n"

#: src/getopt.c:741 src/getopt.c:914
#, c-format
msgid "%s: option `%s' requires an argument\n"
msgstr "%s: `%s' 옵션은 인수가 필요합니다\n"

#: src/getopt.c:770
#, c-format
msgid "%s: unrecognized option `--%s'\n"
msgstr "%s: 인식할 수 없는 옵션 `--%s'\n"

#: src/getopt.c:774
#, c-format
msgid "%s: unrecognized option `%c%s'\n"
msgstr "%s: 인식할 수 없는 옵션 `%c%s'\n"

#: src/getopt.c:800
#, c-format
msgid "%s: illegal option -- %c\n"
msgstr "%s: 잘못된 옵션 -- %c\n"

#: src/getopt.c:803
#, c-format
msgid "%s: invalid option -- %c\n"
msgstr "%s: 부적절한 옵션 -- %c\n"

#: src/getopt.c:833 src/getopt.c:963
#, c-format
msgid "%s: option requires an argument -- %c\n"
msgstr "%s: 인수가 필요한 옵션입니다 -- %c\n"

#: src/getopt.c:880
#, c-format
msgid "%s: option `-W %s' is ambiguous\n"
msgstr "%s: `-W %s'는 모호한 옵션입니다\n"

#: src/getopt.c:898
#, c-format
msgid "%s: option `-W %s' doesn't allow an argument\n"
msgstr "%s: `-W %s' 옵션은 인수를 허용하지 않습니다\n"

#: src/hello.c:164
msgid "Too many arguments\n"
msgstr "인수가 너무 많습니다\n"

#: src/hello.c:165
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "더 많은 정보를 보려면 `%s --help' 하십시오.\n"

#. TRANSLATORS: --help output 1
#. no-wrap
#: src/hello.c:177
msgid "GNU hello, THE greeting printing program.\n"
msgstr "GNU hello, 인사말을 출력하는 바로 \"그\" 프로그램입니다.\n"

#. TRANSLATORS: --help output 2
#. no-wrap
#: src/hello.c:182
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "사용법: %s [옵션]\n"

#. TRANSLATORS: --help output 3 : options 1/2
#. no-wrap
#: src/hello.c:188
msgid ""
"  -h, --help          display this help and exit\n"
"  -v, --version       display version information and exit\n"
msgstr ""
"  -h, --help          이 도움말을 출력하고 마칩니다\n"
"  -v, --version       버전 정보를 표시하고 마칩니다\n"

#. TRANSLATORS: --help output 4 : options 2/2
#. no-wrap
#: src/hello.c:195
msgid ""
"  -t, --traditional       use traditional greeting format\n"
"  -n, --next-generation   use next-generation greeting format\n"
"  -m, --mail              print your mail\n"
msgstr ""
"  -t, --traditional       전통적인 인사말 형식을 사용합니다\n"
"  -n, --next-generation   신세대 인사말 형식을 사용합니다\n"
"  -m, --mail              받은 편지를 표시합니다\n"

#. TRANSLATORS: --help output 5 (end)
#. TRANSLATORS, please don't forget to add the contact address for
#. your translation!
#. no-wrap
#: src/hello.c:205
#, c-format
msgid "Report bugs to <%s>.\n"
msgstr "버그를 %s로(으로) 알려 주세요.\n"

#: src/hello.c:217
#, c-format
msgid ""
"Copyright (C) %s Free Software Foundation, Inc.\n"
"There is NO warranty; not even for MERCHANTABILITY or FITNESS FOR A\n"
"PARTICULAR PURPOSE.  You may redistribute copies of GNU %s under the terms\n"
"of the GNU General Public License.\n"
"For more information about these matters, see the file named COPYING.\n"
msgstr ""
"Copyright (C) %s Free Software Foundation, Inc.\n"
"상품성이나 특정 목적에 대한 적합성을 비롯하여, 어떠한 보증도 하지 \n"
"않습니다.  GNU %s는 GNU General Public License에 따라 복사본을\n"
"재배포할 수 있습니다.\n"
"이에 관한 더 자세한 정보는, COPYING이라는 이름의 파일을 보십시오.\n"

#: src/hello.c:228
#, c-format
msgid "%s: Incompatible flags: -m and -t\n"
msgstr "%s: 동시에 사용할 수 없는 옵션: -m과 -t\n"

#: src/hello.c:260
#, c-format
msgid "%s: Who are you?\n"
msgstr "%s: 당신은 누구세요?\n"

#: src/hello.c:282
#, c-format
msgid "%s: Cannot find your mail spool file.\n"
msgstr "%s: 편지 저장 파일을 찾지 못했습니다.\n"

#: src/hello.c:326
msgid "Nothing happens here."
msgstr "여기선 아무 일도 일어나지 않습니다."

#: src/hello.c:332
msgid "hello, world\n"
msgstr "안녕, 여러분\n"

