#, c-format
msgid "%s: Incompatible flags: -m and -t\n"
msgstr "%s: Opcions incompatibles: -m i -t\n"

#: src/hello.c:260
#, c-format
msgid "%s: Who are you?\n"
msgstr "%s: Quí ets?\n"

#: src/hello.c:282
#, c-format
msgid "%s: Cannot find your mail spool file.\n"
msgstr "%s: No es pot trobar el teu arxiu del spool de correu.\n"

#: src/hello.c:326
