#: src/getopt.c:693
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: opcio '%s' estas plursenca\n"

#: src/getopt.c:718
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: opcio '--%s' ne permesas argumenton\n"

